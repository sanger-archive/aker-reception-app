<% content_for :main_title do %>
  Material Submission
<% end %>
<%= render :partial => 'title_buttons' %>
<div class="row">
  <div class="col-md-12">
    <%= link_to 'Create New Submission', new_material_submission_url, class: 'btn btn-success vcenter' %>
  </div>
</div>

<div class="row">
  <div class="col-md-12">
    <h2>Incomplete Submissions</h2>
    <%= render "material_submission_table", material_submissions: @pending_material_submissions %>
  </div>
</div>

<div class="row">
  <div class="col-md-12">
    <h2>Completed Submissions</h2>
    <%= render "material_submission_table", material_submissions: @active_material_submissions %>
  </div>
</div>
