    "taxon_id": {
      "show_on_form": true,
      "searchable": true,
      "required": true,
      "friendly_name": "Taxon ID",
      "field_name_regex": "^taxon(?:[-_ ]*id)?$",
      "show_on_set_results": true,
      "type": "string"
