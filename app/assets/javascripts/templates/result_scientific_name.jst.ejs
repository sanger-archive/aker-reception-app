<div class='row'>
  <div class='col-xs-6'><span class='scientific-name'> <%= text %> </span></div>
  <div class='col-xs-6'>
    <div class='pull-right'><span class='tax-id '><%= taxId %></span> <span class='badge badge-secondary'>ID</span></div>
  </div>
</div>
<div class='row'>
  <div class='col-xs-6'>
    <% if (typeof commonName != 'undefined') { %>
      <span class='hidden badge badge-secondary'>Cn</span> <span class='common-name'> <%= commonName %></span>
    <% } %>
  </div>
  <div class='col-xs-6'>
    <% if (typeof displayName != 'undefined') { %>
      <span class='hidden badge badge-secondary'>Dn</span> <span class='common-name'> <%= displayName %> </span>
    <% } %>
  </div>
</div>
