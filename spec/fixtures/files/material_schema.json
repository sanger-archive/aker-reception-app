  "searchable": [
    "is_tumour",
    "scientific_name",
    "submitter_id",
    "concentration",
    "phenotype",
    "taxon_id",
    "owner_id",
    "available",
    "date_of_receipt",
    "volume",
    "hmdmc",
    "tissue_type",
    "donor_id",
    "supplier_name",
    "gender",
    "amount"
  ]
